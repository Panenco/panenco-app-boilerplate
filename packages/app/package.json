{
  "name": "@pma/app",
  "version": "1.0.0",
  "author": "Panenco <info@panenco.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.development.js",
    "start:development": "cross-env PROJECT_ENV=development npm run start",
    "start:staging": "cross-env PROJECT_ENV=staging npm run start",
    "start:production": "cross-env PROJECT_ENV=production npm run start",
    "build": "webpack --config config/webpack.config.production.js",
    "build:development": "cross-env BABEL_ENV=production PROJECT_ENV=development npm run build",
    "build:staging": "cross-env BABEL_ENV=production PROJECT_ENV=staging npm run build",
    "build:production": "cross-env BABEL_ENV=production PROJECT_ENV=production npm run build",
    "clean": "rimraf dist",
    "analyzer": "cross-env NODE_ENV=production BABEL_ENV=production PROJECT_ENV=production webpack --config config/webpack.config.production.js --analyze",
    "lint:js": "eslint config src --ext .js --ext .jsx",
    "lint:css": "stylelint 'src/**/*.scss'",
    "prettier": "prettier '{config,src}/**/*.{js,jsx}' --write",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js --fix --"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@loadable/component": "^5.11.0",
    "@pma/ui": "^1.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.5",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.0",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^4.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "svg-sprite-loader": "^4.2.1",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
